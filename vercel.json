{
  "buildCommand": "cd build-template/src/frontend && npm run build:skip-bindings",
  "outputDirectory": "build-template/src/frontend/dist",
  "installCommand": "npm install --prefix build-template/src/frontend",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
